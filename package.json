{
  "name": "eslint-plugin-jshow-mono",
  "version": "1.0.5",
  "license": "MIT",
  "description": "ESLint plugin for jShow",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "jshow",
    "eslint-plugin-jshow"
  ],
  "homepage": "https://github.com/j-show/eslint-plugin-jshow",
  "repository": {
    "type": "git",
    "url": "https://github.com/j-show/eslint-plugin-jshow.git"
  },
  "bugs": {
    "url": "https://github.com/j-show/eslint-plugin-jshow/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "precommit": "lint-staged",
    "build": "ts-node -P ./scripts/tsconfig.json -T ./scripts/publish",
    "clean": "rm -rf ./dist && npm run clean:rules",
    "clean:rules": "npm run clean --workspace=eslint",
    "test:rules": "npm run test --workspace=eslint",
    "test:react": "npm run test --workspace=react",
    "test:vue": "npm run test --workspace=vue",
    "preview:react": "npm run preview --workspace=react",
    "preview:vue": "npm run preview --workspace=vue"
  },
  "engines": {
    "pnpm": ">=8"
  },
  "workspaces": [
    "packages/*"
  ],
  "peerDependencies": {
    "prettier": ">=3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "chalk": "^4.1.0",
    "execa": "^4.0.2",
    "fs-extra": "^11.2.0",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
